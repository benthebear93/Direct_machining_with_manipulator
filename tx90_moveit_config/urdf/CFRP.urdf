        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </collision>
    </link> -->
    <link name="top_part"/>
    <joint name="$cfrp_joint" type="fixed">
        <parent link="babbbbse_link"/>
        <child link="top_part"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>
</robot>